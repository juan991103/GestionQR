@model IEnumerable<GestionQR.Models.Departamentos>

@{
    ViewBag.Title = "Index";
}

<h2>Departamento</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tipo_departamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.encargado_departamento)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_departamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.encargado_departamento)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
